<div class="row">
    <div class="col-5 header-col">
        Nimetus
    </div>
    <div class="col header-col">
        Toimumisaeg
    </div>
    <div class="col header-col">
        Toimumise Koht
    </div>
    <div class="col-1 header-col">
        Osavõtjad
    </div>
    <div class="col-2 header-col">
    </div>
</div>

@foreach (var item in ViewBag.PlaneeritudUritused)
{
    <div class="row">
        <div class="col-5 my-col">
            @item.Nimi
        </div>
        <div class="col my-col">
            @item.Toimumisaeg
        </div>
        <div class="col my-col">
            @item.ToimumiseKoht
        </div>
        <div class="col-1 my-col">
            @item.OsavotjateArv
        </div>
        <div class="col-2 my-col">
            <button type="button" class="btn btn-danger" onclick="EemaldaUritus(@item.Id)">Tühista</button>
        </div>
    </div>
}